{
  "name": "play-with-mongoose",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node index.ts",
    "test": "eslint --ext .js,.ts . && jest"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fs": "^0.0.1-security",
    "jest": "^25.1.0",
    "mongoose": "^5.10.7",
    "prettier": "^2.0.4",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "ignorePatterns": [
      "node_modules/",
      "*.test.ts",
      "mongo-init.js"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 1,
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testEnvironment": "node"
  }
}
